{
  "name": "shopping-list",
  "version": "0.0.0",
  "description": "A shopping list web app",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "lint": "npm run lint:es",
    "lint:es": "eslint --ext .js --ext .jsx .",
    "prebuild": "npm run clean",
    "build": "npm run build:static; npm run build:js",
    "build:static": "NODE_ENV='production' webpack --config webpack.prod.js",
    "build:js": "babel src -d dist",
    "server": "NODE_ENV='production' NODE_PATH=dist node dist/server.js",
    "prestart": "npm run build",
    "start": "npm run server",
    "dev": "nodemon",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "author": "Mike Cravey <techie@craveytrain.com>",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "rmwc": "^0.0.1-rc11",
    "serialize-javascript": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "deep-freeze": "^0.0.1",
    "eslint": "^4.13.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^21.2.1",
    "nodemon": "^1.13.3",
    "postcss-loader": "^2.0.9",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.1"
  }
}
